<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>DiffGold</id>
    <title>DiffGold</title>
    <version>1.0.1</version>
    <authors>Matt Ellis</authors>
    <owners>Matt Ellis</owners>
    <summary>Diff results from ReSharper's acceptance test framework in Visual Studio's diff viewer</summary>
    <description>Adds buttons to the unit test session toolbar to edit and diff results files from ReSharper's acceptance test framework. Diffs are shown using Visual Studio's built in diff viewer</description>
    <releaseNotes>&#8226; Handles spaces in path names</releaseNotes>
    <projectUrl>https://github.com/citizenmatt/resharper-diffgold</projectUrl>
    <licenseUrl>https://raw.github.com/citizenmatt/resharper-diffgold/master/license.txt</licenseUrl>
    <iconUrl>https://raw.github.com/citizenmatt/resharper-diffgold/master/gold_icon.png</iconUrl>
    <copyright>Copyright 2014 Matt Ellis</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="ReSharper" version="[8.2,8.3)" />
    </dependencies>
    <tags>resharper diff gold sdk</tags>
  </metadata>
  <files>
    <file src="..\src\resharper-diffgold\bin\Release\CitizenMatt.ReSharper.DiffGold.8.2.dll"
          target="ReSharper\v8.2\vs11.0\plugins" />
    <file src="..\src\resharper-diffgold\bin\Release\CitizenMatt.ReSharper.DiffGold.8.2.dll"
          target="ReSharper\v8.2\vs12.0\plugins" />
  </files>
</package>
